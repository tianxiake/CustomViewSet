<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--自定义属性-->
    <declare-styleable name="MyTextView">
        <attr name="text_interval" format="integer" />
    </declare-styleable>

    <!--自定义的简易相对布局属性-->
    <declare-styleable name="CustomSimpleRelativeLayout">

        <attr name="layout_position">
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
            <enum name="center" value="3"/>
            <enum name="bottom" value="4"/>
            <enum name="rightAndBottom" value="5"/>
        </attr>
    </declare-styleable>
</resources>
